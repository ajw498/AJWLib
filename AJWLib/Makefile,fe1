HEADERS	=	\
		Draw.h\
		DrawFile.h\
		Error2.h\
		File.h\
		FilerActn.h\
		Flex.h\
		Icon.h\
		Menu.h\
		Msgs.h\
		Pane.h\
		Str.h\
		TaskWindow.h\
		Window.h\
		Assert.h\
		MemCheckFlex.h\
		Font.h

OBJECTS	=	\
		Draw.o\
		Drawfile.o\
		Error2.o\
		File.o\
		FilerActn.o\
		Flex.o\
		Icon.o\
		Menu.o\
		Msgs.o\
		Pane.o\
		Str.o\
		TaskWindow.o\
		Window.o\
		MemCheckFlex.o\
		Font.o

INCLUDE	=	L:

TARGET		=	AJWLib.o

CCFLAGS		=	-I$(INCLUDE) -mstubs -mthrowback -Wall -O3
LINKFLAGS	=-s

CC		=	gcc -c $(CCFLAGS)
LINK		=	MakeALF $(LINKFLAGS)

.c.o: $(HEADERS)
	$(CC) $< -o $@


$(TARGET):	$(OBJECTS)
	$(LINK) -o $@ $(OBJECTS)

Drawfile.o:
	copy Drawfile.l.drawfile.0x45540.o.000 o.Drawfile A~C~DF~L~N~P~QRS~T~V

MemCheckFlex.o: MemCheckFlex.c MemCheckFlex.h
	cc -jC: -IL: -throwback -fah -Otime -c MemCheckFlex.c -o MemCheckFlex.o

Error2.o: Error2.c Error2.h
	cc -jC: -IL: -throwback -fah -Otime -c Error2.c -o Error2.o
