
OBJECTS	=	\
		Draw.o\
		Drawfile.o\
		Error2.o\
		File.o\
		FilerActn.o\
		Flex.o\
		Icon.o\
		Menu.o\
		Msgs.o\
		Pane.o\
		Str.o\
		TaskWindow.o\
		Window.o\
		MemCheckFlex.o\
		Font.o

MCOBJECTS	=	\
		mc.Draw\
		mc.Drawfile\
		mc.Error2\
		mc.File\
		mc.FilerActn\
		mc.Flex\
		mc.Icon\
		mc.Menu\
		mc.Msgs\
		mc.Pane\
		mc.Str\
		mc.TaskWindow\
		mc.Window\
		mc.MemCheckFlex\
		mc.Font

INCLUDE	=	C:

.SUFFIXES: .c .o .mc

CCFLAGS		=	-I$(INCLUDE) -depend !Depend -throwback -strict -fah -Otime
LINKFLAGS	=-s

CC		=	cc -c $(CCFLAGS)
LINK		=	MakeALF $(LINKFLAGS)

all: AJWLib.o mc.AJWLib

.c.o:
	$(CC) $< -o $@

.c.mc:
	MemCheck.cc $(CC) -g $< -o $@

AJWLib.o:	$(OBJECTS)
	$(LINK) -o $@ $(OBJECTS)

mc.AJWLib:	$(MCOBJECTS)
	$(LINK) -o $@ $(MCOBJECTS)

Drawfile.o:
	copy Drawfile.l.drawfile.0x45540.o.000 o.Drawfile A~C~DF~L~N~P~QRS~T~V

mc.Drawfile:
	copy Drawfile.l.drawfile.0x45540.o.000 mc.Drawfile A~C~DF~L~N~P~QRS~T~V

MemCheckFlex.o: MemCheckFlex.c MemCheckFlex.h
	cc -jC: -IL: -throwback -fah -Otime -c MemCheckFlex.c -o MemCheckFlex.o

mc.MemCheckFlex: MemCheckFlex.c MemCheckFlex.h
	cc -jC: -IL: -throwback -fah -Otime -c MemCheckFlex.c -o mc.MemCheckFlex

mc.Flex: Flex.c Flex.h
	cc -jC: -IL: -throwback -fah -Otime -c Flex.c -o mc.Flex

Error2.o: Error2.c Error2.h
	cc -jC: -IL: -throwback -fah -Otime -c Error2.c -o Error2.o

