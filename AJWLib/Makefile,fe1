INCLUDE	=	L:

TARGET		=	AJWLib.o

CCFLAGS		=	-I$(INCLUDE) -mthrowback -O3
LINKFLAGS	=-s

CC		=	gcc -c $(CCFLAGS) -mstubs
LINK		=	MakeALF $(LINKFLAGS)

.c.o: $(HEADERS)
	$(CC) $< -o $@


$(TARGET):	$(OBJECTS)
	$(LINK) -o $@ $(OBJECTS)

